{
  "name": "chat.app.privacysafe.io",
  "version": "0.5.11",
  "license": "ISC",
  "private": true,
  "type": "module",
  "scripts": {
    "clean:coverage": "rimraf coverage",
    "dev": "vite",
    "build:deno": "deno bundle './src-deno/chats-deno.ts' -- './app/chatsDenoServices.js' && deno bundle './src-deno/chat-delivery-deno.ts' -- './app/deliveryDenoServices.js'",
    "build": "vue-tsc --noEmit && vite build && npm run build:deno",
    "serve": "vite preview",
    "stylelint": "stylelint --config stylelint.config.js 'src/**/*.{vue,scss}'",
    "stylelint:fix": "stylelint --config stylelint.config.js --fix 'src/**/*.{vue,scss}'",
    "linter": "eslint -c .eslintrc.js --ext .js,.vue,.ts, src",
    "linter:fix": "eslint -c .eslintrc.js --fix --ext .js,.vue,.ts, src",
    "lint": "yarn linter && yarn stylelint",
    "lint:fix": "yarn linter:fix && yarn stylelint:fix",
    "test": "yarn clean:coverage && jest",
    "test:watch": "yarn clean:coverage && jest --watch"
  },
  "dependencies": {
    "@v1nt1248/3nclient-lib": "^0.0.38",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.4.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^7.0.0",
    "@types/jasmine": "^4.3.1",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.199",
    "@types/node": "^16.18.16",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/devtools": "6.5.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vuedx/typescript-plugin-vue": "^0.7.6",
    "eslint": "^8.50.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "jest": "^29.5.0",
    "jest-serializer-vue": "^3.1.0",
    "jest-transform-stub": "^2.0.0",
    "postcss-html": "^1.5.0",
    "sass": "^1.68.0",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-rscss": "^0.4.0",
    "stylelint-scss": "^4.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.0.11",
    "vue-jest": "^3.0.7",
    "vue-tsc": "^1.6.5"
  }
}
