{
  "name": "chat.app.privacysafe.io",
  "version": "0.5.3",
  "license": "ISC",
  "private": true,
  "scripts": {
    "clean:coverage": "rimraf coverage",
    "dev": "vite",
    "build:deno": "deno bundle './src-deno/chats-deno.ts' -- './app/chatsDenoServices.js' && deno bundle './src-deno/chat-delivery-deno.ts' -- './app/deliveryDenoServices.js'",
    "build": "vue-tsc --noEmit && vite build && npm run build:deno",
    "serve": "vite preview",
    "stylelint": "stylelint --config stylelint.config.js 'src/**/*.{vue,scss}'",
    "stylelint:fix": "stylelint --config stylelint.config.js --fix 'src/**/*.{vue,scss}'",
    "linter": "eslint -c .eslintrc.js --ext .js,.vue,.ts, src",
    "linter:fix": "eslint -c .eslintrc.js --fix --ext .js,.vue,.ts, src",
    "lint": "yarn linter && yarn stylelint",
    "lint:fix": "yarn linter:fix && yarn stylelint:fix",
    "test": "yarn clean:coverage && jest",
    "test:watch": "yarn clean:coverage && jest --watch"
  },
  "dependencies": {
    "@iconify-icons/ic": "^1.2.13",
